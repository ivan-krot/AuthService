<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="full" parallel="methods" thread-count="5" data-provider-thread-count="3">
    <listeners>
        <listener class-name="net.absoft.listeners.AnnotationTransformer"/>
    </listeners>
    <test name="authServiceTest">
        <parameter name="email-address" value="user2@test.com"/>
        <parameter name="password" value="password2"/>
        <groups>
            <define name="all">
                <include name="negative"/>
                <include name="positive"/>
            </define>
            <run>
                <include name="all"/>
            </run>
        </groups>
    </test>
    <packages>
        <package name="net.absoft"/>
    </packages>
</suite>